//  https://leetcode-cn.com/problems/two-sum-ii-input-array-is-sorted
/**
 * 暴力遍历啦
 */
int* twoSum(int* numbers, int numbersSize, int target, int* returnSize){
    int *ans = malloc(2*sizeof(int));
    *returnSize = 2;
    int i = 0, j = 1;
    while(numbers[i] + numbers[j] != target){
        if(j == numbersSize-1){
            i++;
            j = i;
        }
        j++;
    }
    ans[0] = i + 1;
    ans[1] = j + 1;
    return ans;
}
