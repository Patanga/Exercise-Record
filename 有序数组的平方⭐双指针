//  https://leetcode-cn.com/problems/squares-of-a-sorted-array
int tmp(int* a, int* b){
    return (*a - *b); 
 }
int* sortedSquares(int* nums, int numsSize, int* returnSize){
    for(int i = 0; i < numsSize; i++){
        nums[i] = nums[i]*nums[i];
        //printf("%i ", nums[i]);
    }
    qsort(nums, numsSize, sizeof(int), tmp);
    *returnSize = numsSize;
    return nums;
}
